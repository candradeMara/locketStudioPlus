{
  "name": "marathon",
  "version": "0.0.1",
  "description": "website",
  "productName": "marathon",
  "author": "AwesomeScott",
  "private": true,
  "scripts": {
    "build": "quasar build --mode pwa",
    "dev": "quasar dev --mode spa",
    "dev:pwa": "quasar dev --mode spa",
    "deploy": "quasar build --mode spa && firebase deploy --only hosting",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:all": "quasar build --mode spa && firebase deploy",
    "emulator": "firebase emulators:start --only auth,functions,firestore,storage --import ./emulatorData --export-on-exit ./emulatorData",
    "lint": "eslint --ext .js,.vue ./",
    "lintfix": "eslint --fix --ext .js,.vue ./",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.9",
    "@vue/compiler-dom": "^3.4.21",
    "add": "^2.0.6",
    "add-to-calendar-button": "^2.6.5",
    "algoliasearch": "^4.22.1",
    "aos": "^2.3.4",
    "canvas2svg": "^1.0.16",
    "dotenv": "^16.4.5",
    "firebase": "^9.23.0",
    "imagekit": "^4.1.4",
    "jszip": "^3.10.1",
    "opentype.js": "^1.3.4",
    "pinia": "^2.1.7",
    "pinia-plugin-persist": "^1.0.0",
    "quasar": "^2.15.0",
